<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">
    <!-- <link rel="stylesheet" type="text/css" href="/calendarStyle.css"> -->
    <link rel="stylesheet" href="/public/css/calendarStyle.css">
    <title></title>
  </head>
  <body>
    <form class="" action="/work" method="post">
      <button style="font-size: 30px; margin-left: 47vw" type="submit"> Back</button>
    </form>
<div class="container">
  <div class="previous">
    <a href="/<%= startingMonth %>/settings/previous"  ><</a>
  </div>
  <div class="year">
    <p><%=startingMonth %>
    <br>
      <%= currentYear %>
    </p>
  </div>
  <div class="next">
    <a href="/<%= startingMonth %>/settings/next"  >></a>
  </div>

  <% week.forEach(function(weekDays){  %>
    <div class="weekDay"><%= weekDays  %></div>
  <% });    %>

<% for(var i=0 ; i<37; i++){ %>
  <% if(i >= startingMonthDay-1){ %>
    <% if(i <= startingMonthDaysAmount+startingMonthDay){ %>
      <% if(i == startingMonthDaysAmount+startingMonthDay-1){ %>
        <% break; %>
      <% } else { %>
         <div class="item"><%= i - count + 1 %></div>
      <% } %>
    <% } %>
  <% } else { %>
    <div class="item"></div>
  <% } %>
<% } %>

</div>

  </body>
</html>
