  <body>
    <form class="" action="/work" method="post">
      <button style="font-size: 30px; margin-left: 47vw" type="submit"> Back</button>
    </form>
<div class="container">

  <div class="previous">
    <a href="/<%= startingMonth %>/settings/previous"  ><</a>
  </div>
  <div class="year">
    <p><%=startingMonth %>
    <br>
      2021
    </p>
  </div>
  <div class="next">
    <a href="/<%= startingMonth %>/settings/next"  >></a>
  </div>

  <% week.forEach(function(weekDays){  %>
    <div class="weekDay"><%= weekDays  %></div>
  <% });    %>

<!-- <%  if(week.length > 0){ %>
    <p>Jest wartość</p>
<%  } %> -->


<% for(var i=0 ; i<37; i++){ %>
  <% if(i >= startingMonthDay-1){ %>
    <% if(i <= startingMonthDaysAmount+startingMonthDay){ %>
      <% if(i == startingMonthDaysAmount+startingMonthDay-1){ %>
        <% break; %>
      <% } else { %>
         <div class="item"><%= i - count + 1 %></div>

      <% } %>
    <% } %>
  <% } else { %>
    <div class="item"></div>
  <% } %>
<% } %>
<%  %>
</div>

    <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
